{
  "name": "stoooreee",
  "version": "1.0.0",
  "description": "midtrans sample store for snap",
  "scripts": {
    "start": "node server.js",
    "build": "npm run wp && npm run ns",
    "wp": "webpack --progress --display-modules --colors",
    "wp:watch": "webpack --progress --display-modules --colors --watch",
    "ns": "node-sass --include-path scss --output-style compressed src/css/base.scss public/stylesheets/style.css",
    "ns:watch": "node-sass --watch --include-path scss src/css/base.scss public/stylesheets/style.css",
    "postinstall": "npm run build",
    "sandbox": "NODE_ENV=sandbox npm start"
  },
  "keywords": [],
  "author": "Dion Dirza",
  "license": "ISC",
  "engines": {
    "node": "4.4.3"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.dev.veritrans.co.id:rubicon/sample-store-ui.git"
  },
  "dependencies": {
    "accounting": "^0.4.1",
    "babel-cli": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "^1.15.0",
    "classnames": "^2.2.3",
    "ejs": "^2.4.1",
    "express": "^4.13.4",
    "node-sass": "^3.4.2",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "request": "^2.72.0",
    "shush": "^1.0.0",
    "static-gzip": "^0.1.6",
    "superagent": "^1.8.3",
    "webpack": "^1.13.0"
  }
}
